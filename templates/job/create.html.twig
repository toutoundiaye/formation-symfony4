{% extends 'base.html.twig' %}

{% block title %} {{ 'job.createJob.title'|trans({}, 'job') }} - {{ parent() }}{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h1>{{ 'job.createJob.title'|trans({}, 'job') }}</h1>
    </div>
    <div>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <br />
        <input type="submit" value="{{ 'job.create'|trans({}, 'job') }}"/>
        {{ form_end(form) }}
    </div>
{% endblock %}